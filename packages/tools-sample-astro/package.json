{
  "name": "@ktam/tools-sample-astro",
  "version": "0.2.1",
  "private": true,
  "description": "A test Astro project to make sure the lint / prettier configs work as expected",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingston/ktam-tools"
  },
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier -w .",
    "preview": "astro preview",
    "start": "astro dev",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@tailwindcss/vite": "4.1.13",
    "astro": "^5.13.9",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@ktam/lint-react": "workspace:*",
    "@types/node": "24.5.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  }
}
